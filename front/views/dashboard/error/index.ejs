<section id="main-content">
    <section id="errors" class="wrapper">
        <!-- page start -->

        <div class="row">
            <div class="col-md-12">
                <!--breadcrumbs start -->
                <ul class="breadcrumb">
                    <li><a href="/projects"><i class="fa fa-home"></i> Home</a></li>
                    <li><a id="load_title" href="/dashboard/<%= data.id%>"><i id="load_icon" class="fa fa-refresh fa-spin"></i></a></li>
                    <li class="active">Errors</li>
                </ul>
                <!--breadcrumbs end -->
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12">
                <section class="panel">
                    <header class="panel-heading">
                        Filter
                        <span class="tools pull-right">
                            <a href="javascript:;" class="fa fa-chevron-down" onclick="$(this).ionRangeSlider('update');"></a>
                        </span>
                    </header>
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-12">
                                <form class="form-horizontal bucket-form" method="get">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Rank</label>

                                        <div class="col-sm-9 icheck">
                                            <div class="flat-red">
                                                <div class="radio" style="padding-left:0">
                                                    <input id="check-rank-All" type="checkbox" checked>
                                                    <label>All</label>
                                                </div>
                                            </div>
                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-rank-unhandled" type="checkbox" checked>
                                                    <label>Unhandled</label>
                                                </div>
                                            </div>
                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-rank-native" type="checkbox" checked>
                                                    <label>Native</label>
                                                </div>
                                            </div>
                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-rank-critical" type="checkbox" checked>
                                                    <label>Critical</label>
                                                </div>
                                            </div>
                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-rank-major" type="checkbox" checked>
                                                    <label>Major</label>
                                                </div>
                                            </div>
                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-rank-minor" type="checkbox" checked>
                                                    <label>Minor</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Date</label>
                                        <div class="col-sm-9">
                                            <input id="date-slider" type="text" value="1;28" data-type="double" data-step="1" data-postfix=" day" data-from-max="1" data-to="1" data-hasgrid="true" />
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Status</label>
                                        <div class="col-sm-9 icheck">
                                            <div class="flat-red">
                                                <div class="radio" style="padding-left:0">
                                                    <input id="check-status-All" type="checkbox" checked>
                                                    <label>All</label>
                                                </div>
                                            </div>

                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-status-new" type="checkbox" checked>
                                                    <label>New</label>
                                                </div>
                                            </div>

                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-status-open" type="checkbox" checked>
                                                    <label>Open</label>
                                                </div>
                                            </div>

                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-status-fixed" type="checkbox" checked>
                                                    <label>Fixed</label>
                                                </div>
                                            </div>

                                            <div class="flat-green">
                                                <div class="radio">
                                                    <input id="check-status-ignore" type="checkbox" checked>
                                                    <label>Ignore</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">APP Version</label>
                                        <div id="select-app" class="col-sm-9 icheck">
                                            <div class="flat-red">
                                                <div class="radio" style="padding-left:0">
                                                    <input id="check-app-All" type="checkbox" checked>
                                                    <label>All</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">OS Version</label>
                                        <div id="select-os" class="col-sm-9 icheck">
                                            <div class="flat-red">
                                                <div class="radio" style="padding-left:0">
                                                    <input id="check-os-All" type="checkbox" checked>
                                                    <label>All</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Country</label>
                                        <div class="col-sm-9">
                                            <div id="countrysearch" class="tagsearch">
                                                <select id="select-country" class="form-control" style="width:100px">
                                                    <option>Select</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Class</label>
                                        <div class="col-sm-9">
                                            <div id="classsearch" class="tagsearch">
                                                <select class="form-control" style="width:100px">
                                                    <option>Select</option>
                                                    <option>android.app.LoadedApk</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>

        <div class="row">
            <div class="col-sm-12">
                <section class="panel">
                    <header class="panel-heading">
                        Error Table
                        <span class="tools pull-right">
                            <a href="javascript:;" class="fa fa-chevron-down"></a>
                         </span>
                    </header>
                    <div class="panel-body">
                        <div>
                            <div class="btn-row">
                                <div class="btn-group" data-toggle="buttons">
                                    <label class="btn btn-info active tooltips" id="btnTranding">
                                        <input type="radio" name="options"><i class="fa fa-exclamation-triangle"></i> Tranding
                                    </label>
                                    <label class="btn btn-info tooltips" id="btnLatest">
                                        <input type="radio" name="options"><i class="fa fa-refresh"></i> Latest
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div id="errors-table" class="adv-table">
                            <table class="table table-hover table-striped" id="dynamic-table">
                                <thead>
                                <tr>
                                    <th style="width:50px">Rank</th>
                                    <th style="width:60px">Count</th>
                                    <th class="auto-size">Name</th>
                                    <th style="width:180px">Tags</th>
                                    <th style="width:60px" class="res-hidden">Status</th>
                                    <th style="width:60px" class="res-hidden">Date</th>
                                </tr>
                                </thead>
                                <tbody>
                                <tr>
                                    <td data-title="Rank" class="text-center"></td>
                                    <td data-title="Count" class="text-center text-danger text-bold"></td>
                                    <td data-title="Name" class="table-text-ellipsis"></td>
                                    <td data-title="Tags" class="tags"></td>
                                    <td data-title="Status" class="res-hidden"></td>
                                    <td data-title="Date" class="res-hidden"></td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </section>
</section>